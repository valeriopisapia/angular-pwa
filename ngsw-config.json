{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api.punkapi.com",
      "version": "1.0",
      "urls": [
        "https://api.punkapi.com/v2/beers"
      ],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    }
  ]
}